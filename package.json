{
  "name": "webpack-cdn-plugin",
  "version": "1.8.1",
  "description": "A webpack plugin that use externals of CDN urls for production and local node_modules for development",
  "main": "main.js",
  "module": "module.js",
  "scripts": {
    "test": "nyc jasmine",
    "install": "babel module.js --out-file main.js",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/van-nguyen/webpack-cdn-plugin.git"
  },
  "files": [
    ".babelrc",
    "module.js",
    "README.md"
  ],
  "keywords": [
    "webpack",
    "html",
    "externals",
    "script",
    "include",
    "assets",
    "plugin",
    "cdn",
    "production",
    "development"
  ],
  "author": {
    "name": "Van Nguyen",
    "email": "van@shirotech.com",
    "url": "https://shirotech.com/"
  },
  "contributors": [
    {
      "name": "Van Nguyen",
      "email": "van@shirotech.com",
      "url": "https://shirotech.com/"
    }
  ],
  "engines": {
    "node": ">=5.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/van-nguyen/webpack-cdn-plugin/issues"
  },
  "homepage": "https://github.com/van-nguyen/webpack-cdn-plugin",
  "devDependencies": {
    "archy": "^1.0.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nyc": "^11.6.0",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.1.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1"
  }
}
