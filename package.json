    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jasmine": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nyc": "^14.1.1",
    "validate-commit-msg": "^2.14.0",
    "webpack": "4.33.0",
